/* 记录商品信息 */
.contents {
	height: 170rpx;
}
.contents .goodsmsg {
	position: absolute;
	margin-left: 190rpx;
	width: 530rpx;
}
.contents .goodsmsg .goodsName {
	overflow: hidden;
	-o-text-overflow: ellipsis;
	   text-overflow: ellipsis;
	white-space: nowrap;
}
.contents .goodsmsg .goodstip {
	height: 24rpx;
	font-size: 24rpx;
	font-family: PingFangSC-Medium, PingFangSC;
	font-weight: 500;
	color: rgba(253, 84, 80, 1);
	line-height: 24rpx;
	margin-top: 18rpx;
}
.contents .goodsmsg .zhong {
	margin-top: 30rpx;
	height: 33rpx;
	font-size: 24rpx;
	font-family: PingFangSC-Medium, PingFangSC;
	font-weight: 500;
	color: rgba(253, 84, 80, 1);
	line-height: 33rpx;
}
.contents .goodsmsg .zhong .right {
	position: absolute;
	right: 100rpx;
	text-align: right;
}
.contents .goodsmsg .zhong .right view {
	margin-left: 10rpx;
	margin-top: -2rpx;
}
.contents .goodsmsg .zhong .right image,
.contents .goodsmsg .zhong .right view {
	float: left;
}
.contents .goodsmsg .zhong .avatar {
	width: 36rpx;
	height: 36rpx;
	border-radius: 50%;
	position: relative;
	top: -4rpx !important;
}
.contents .goodsmsg .zhong .nicknames {
	color: rgba(107, 115, 122, 1);
	max-width: 120rpx;
	overflow: hidden;
	-o-text-overflow: ellipsis;
	   text-overflow: ellipsis;
	white-space: nowrap;
	position: relative;
	height: 40rpx;
	line-height: 40rpx;
}
.contents .goodsmsg .zhong .pzsp {
	height: 40rpx;
	line-height: 40rpx;
	position: absolute;
	width: 100rpx;
	right: -100rpx;
}
.contents .goodspic {
	width: 140rpx;
	height: 140rpx;
	background: rgba(216, 216, 216, 1);
	border-radius: 4rpx;
	margin-left: 30rpx;
}

