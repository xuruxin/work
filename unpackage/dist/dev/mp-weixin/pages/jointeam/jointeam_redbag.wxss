/* 参团信息 */
.msg1,
.msg2,
.msg3 {
	width: 100%;
	text-align: center;
}
.msg1 {
	height: 52rpx;
	font-size: 52rpx;
	font-family: PingFangSC-Medium, PingFangSC;
	font-weight: 500;
	color: #FFFFFF;
	line-height: 52rpx;
	margin-top: 60rpx;
}
.msg1 text {
	font-size: 58rpx;
	color: #FFF100;
	margin: 0 12rpx;
}
.msg2 {
	height: 28rpx;
	font-size: 28rpx;
	font-family: PingFangSC-Regular, PingFangSC;
	font-weight: 400;
	color: #FFFFFF;
	line-height: 28rpx;
	margin-top: 20rpx;
}
.msg3 {
	height: 28rpx;
	font-size: 28rpx;
	font-family: PingFangSC-Regular, PingFangSC;
	font-weight: 400;
	color: #FFFFFF;
	line-height: 28rpx;
	margin-top: 20rpx;
}
.msg3 text {
	background: #FFF100;
	color: #FD5450;
	padding: 2rpx 6rpx;
	margin: 0 10rpx;
	border-radius: 6rpx;
}
.sharebtn {
	width: 670rpx;
	height: 112rpx;
	line-height: 112rpx;
	border-radius: 49rpx;
	font-size: 32rpx;
	font-family: PingFangSC-Medium, PingFangSC;
	font-weight: 500;
	color: rgba(255, 255, 255, 1);
	position: absolute;
	margin-top: 456rpx !important;
	z-index: 1;
	margin-left: 40rpx !important;
}
.sharebtn image {
	width: 100%;
	height: 100%;
}
.avatarBox {
	text-align: center;
	margin-top: 58rpx;
}
.avatarBox .avatar {
	width: 112rpx;
	height: 112rpx;
	display: inline-block;
	text-align: center;
	margin: 0 29rpx;
}
.avatarBox .noavatar {
	border: none !important;
}
.avatarBox .avatar .txt {
	width: 72rpx;
	height: 32rpx;
	line-height: 32rpx;
	background: #FBED00;
	border-radius: 16px;
	position: absolute;
	margin-top: -20rpx;
	font-size: 22rpx;
	font-family: PingFangSC-Medium, PingFangSC;
	font-weight: 500;
	color: #FD5450;
	margin-left: 20rpx;
}
.avatarBox .avatar image {
	width: 112rpx;
	height: 112rpx;
	border-radius: 50%;
	border: 3rpx solid rgba(253, 84, 80, 1);
}
