/* 我的钱包--订单 */
.whitebox {
	margin-left: 40rpx;
	margin-top: 40rpx;
	width: 670rpx;
	height: 246rpx;
	background: rgba(255, 255, 255, 1);
	-webkit-box-shadow: 0rpx 4rpx 30rpx 0rpx rgba(3, 3, 44, 0.09);
	        box-shadow: 0rpx 4rpx 30rpx 0rpx rgba(3, 3, 44, 0.09);
	border-radius: 12rpx;
	position: relative;
	z-index: 1;
}
.whitebox .moneytitle {
	width: 160rpx;
	height: 92rpx;
	line-height: 92rpx;
	font-size: 28rpx;
	font-family: PingFangSC-Medium;
	font-weight: 700;
	color: rgba(62, 62, 80, 1);
}
.whitebox .tixianbox {
	height: 92rpx;
	border-bottom: 1px solid #f4f4f4;
	line-height: 92rpx;
	width: 590rpx;
	margin-left: auto;
	margin-right: auto;
}
.whitebox .money {
	height: 92rpx;
	position: absolute;
	margin-left: 160rpx;
	font-size: 42rpx;
	font-family: DIN-Medium;
	font-weight: 500;
	color: #FD5450;
}
.whitebox .money text {
	font-size: 24rpx !important;
}
.whitebox .tixianbox .tixianbtn {
	position: absolute;
	right: 30rpx;
	margin-top: 20rpx !important;
	width: 162rpx;
	height: 52rpx;
	line-height: 52rpx;
	border-radius: 26rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	    -ms-flex-pack: center;
	        justify-content: center;
	color: #FD5450 !important;
	border: 3rpx solid rgba(253, 84, 80, 1);
}
.tixianbtn>image {
	width: 22rpx;
	height: 24rpx;
	margin-right: 4rpx;
}
.tixianbtn>text {
	font-size: 22rpx;
	font-family: PingFangSC-Medium;
	font-weight: 500;
}
.whitebox .list {
	width: 590rpx;
	margin-left: 40rpx;
	margin-top: 15rpx;
}
.whitebox .list .msg {
	width: 130rpx;
	height: 42rpx;
	font-size: 26rpx;
	font-family: PingFangSC-Regular;
	font-weight: 400;
	color: rgba(51, 51, 61, 1);
	line-height: 42rpx;
}
.whitebox .list .money2 {
	position: absolute;
	right: 80rpx;
	width: 590rpx;
	font-size: 32rpx;
	font-family: DIN-Medium;
	font-weight: 500;
	color: #FD5450;
	line-height: 39rpx;
	text-align: right;
}
.whitebox .list .money2 image {
	width: 17rpx;
	height: 25rpx;
	margin-left: 7rpx;
}
.whitebox .list text {
	font-size: 20rpx;
	margin-left: 4rpx;
}
.whitebox .shouyi>.newlist:nth-child(1) {
	margin-left: 10rpx !important;
}
.whitebox .shouyi>.newlist:nth-child(5) {
	margin-left: 26rpx !important;
}
.newlist {
	float: left;
	width: 156rpx;
	text-align: center;
}
.newlist .text {
	font-size: 24rpx;
	font-weight: 400;
	color: rgba(145, 145, 158, 1);
	margin-top: 10rpx;
	font-family: PingFangSC-Regular;
}
.newlist .text image {
	width: 12rpx;
	height: 22rpx;
	position: relative;
	top: 3rpx;
	left: 4rpx;
}
.newlist image {
	width: 60rpx;
	height: 60rpx;
}
.orderline {
	width: 2rpx;
	height: 60rpx;
	background: -webkit-gradient(linear, left top, left bottom, from(rgba(107, 115, 122, 0)), color-stop(53%, rgba(107, 115, 122, 0.5)), to(rgba(107, 115, 122, 0)));
	background: -o-linear-gradient(top, rgba(107, 115, 122, 0) 0%, rgba(107, 115, 122, 0.5) 53%, rgba(107, 115, 122, 0) 100%);
	background: linear-gradient(180deg, rgba(107, 115, 122, 0) 0%, rgba(107, 115, 122, 0.5) 53%, rgba(107, 115, 122, 0) 100%);
	position: absolute;
	margin-left: 486rpx;
}

