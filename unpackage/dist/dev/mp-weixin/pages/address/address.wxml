<view><cu-custom vue-id="1" bgColor="bg-gradual-redcolor" isBack="{{true}}" bind:__l="__l" vue-slots="{{['content']}}"><view slot="content">地址管理</view></cu-custom><block wx:for="{{datas}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="list"><view class="updbtn" data-jumptype="1" data-closepage="1" data-url="{{'/pages/address/add_address/add_address?address_id='+item.id}}" data-event-opts="{{[['tap',[['jumpTo',['$event']]]]]}}" bindtap="__e">编辑</view><view id="{{item.id}}" data-event-opts="{{[['tap',[['set_address_id',['$event']]]]]}}" bindtap="__e"><view class="people">{{item.name+''}}<text>{{item.phone}}</text></view><view class="address">{{item.province+" "+item.city+" "+item.area}}</view><view class="addxq">{{item.address}}</view></view></view></block><block wx:if="{{datas==''}}"><view class="nodata"><image src="/static/icon_kongyemian_shouhuodizhi.png"></image><view>请点击下方添加收货地址</view></view></block><view style="height:130rpx;"></view><view class="tianjia"><form report-submit data-jumptype="1" data-closepage="1" data-url="/pages/address/add_address/add_address" data-event-opts="{{[['submit',[['jumpTo',['$event']]]]]}}" bindsubmit="__e"><button form-type="submit"><text>+</text>添加收货地址</button></form><form report-submit data-url="/pages/address/add_address/add_address" data-event-opts="{{[['submit',[['choosewxaddress',['$event']]]]]}}" bindsubmit="__e"><button form-type="submit"><text>+</text>获取微信地址</button></form></view></view>