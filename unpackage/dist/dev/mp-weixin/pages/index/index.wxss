
page {
	background: #FFFFFF;
	font-family: DIN-Medium, DIN;
}
.topbox {
	background: #FD5450;
	border-radius: 0rpx 0rpx 32rpx 32rpx;
	text-align: center;
	padding-bottom: 124rpx;
}
.topbox .shuomingBtn {
	width: 690rpx;
	height: 100rpx;
	position: absolute;
	z-index: 8;
	margin-left: 30rpx;
	margin-top: 186rpx;
}
.topbox .videoBtn {
	width: 104rpx;
	height: 102rpx;
	position: absolute;
	right: 28rpx;
	margin-top: 182rpx;
	z-index: 9;
}
.topbox .shuomingtitle {
	width: 692rpx;
	height: 276rpx;
	margin-top: 10rpx;
}
.topbox .searchicon {
	position: absolute;
	width: 27rpx;
	height: 29rpx;
	right: 80rpx;
	margin-top: 30rpx;
}
.topbox .searchinput {
	width: 690rpx;
	height: 88rpx;
	border-radius: 44px;
	opacity: 0.90;
	background: #FFFFFF;
	text-align: left;
	margin-left: 30rpx;
	padding: 0 100rpx 0 40rpx;
	margin-top: 40rpx;
}
.topbox .searchinput input {
	height: 88rpx;
}
.topbox .tips {
	width: 680rpx;
	margin-left: 35rpx;
	height: 50rpx;
	line-height: 50rpx;
	position: absolute;
	margin-top: 40rpx;
}
.topbox .tips view {
	display: inline-block;
	font-size: 24rpx;
	font-family: PingFangSC-Medium, PingFangSC;
	font-weight: 500;
	color: rgba(255, 255, 255, 1);
	width: 100%;
	position: absolute;
	left: 0;
}
.topbox .tips>view:first-child {
	text-align: left;
	margin-left: 20rpx;
}
.topbox .tips>view:nth-child(3) {
	text-align: right;
	right: 0;
}

/* 分类样式 */
.cat_goodstypes {
	padding-top: 0rpx;
	padding-bottom: 0rpx;
	width: 750rpx;
	white-space: nowrap;
	overflow: auto;
	height: 88rpx;
	z-index: 990;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	line-height: 88rpx;
	border-bottom: 1rpx solid rgba(245, 245, 245, 1);
	margin-top: 10rpx;
}
.cat_goodstypes .cat_active {
	font-size: 36rpx;
	font-weight: 500;
	color: rgba(253, 84, 80, 1);
	text-align: center;
}
.cat_goodstype {
	padding: 0 21rpx;
	display: inline-block;
	line-height: 45rpx;
	text-align: center;
	font-size: 30rpx;
	font-family: PingFangSC-Medium;
	font-weight: 500;
	color: rgba(253, 84, 80, 1);
}
.cat_goodstype .cat_red {
	width: 24rpx;
	height: 6rpx;
	background: #FD5450;
	border-radius: 3px;
	margin: auto;
}

/* 分类样式 */

