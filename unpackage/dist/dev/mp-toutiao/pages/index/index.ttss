	width: 100%;
	position: fixed;
	top: 0;
	z-index: 10;
	bottom: 0;
	height: 100%;
	background: #FD5450;
}
.shuomingpic image {
	margin-top: ;
	position: absolute;
	width: 94%;
	margin-left: 3%;
}
.shuomingpic .closeShuoming {
	position: absolute;
	height: 100rpx;
	width: 80%;
	margin-left: 10%;
	z-index: 11;
	margin-top: 1240rpx;
}
