
page {
	background: rgba(255, 171, 184, 1);
}
.header {
	width: 100%;
	height: 1156rpx;
}
image {
	width: 100%;
	height: 100%;
}
.wares {
	width: 100%;
	height: 440rpx;
	position: relative;
}
.explain {
	width: 630rpx;
	height: 360rpx;
	background: rgba(255, 75, 102, 1);
	border-radius: 24rpx;
	position: absolute;
	top: 50%;
	left: 50%;
	-webkit-transform: translate(-50%, -50%);
	        transform: translate(-50%, -50%);
	padding: 0rpx 30rpx;
	box-sizing: border-box;
}
.title {
	font-size: 38rpx;
	font-family: PingFangSC-Medium, PingFang SC;
	font-weight: 500;
	color: rgba(255, 255, 255, 1);
	line-height: 53rpx;
	text-align: center;
	display: block;
	margin-top: 34rpx;
}
.rich {
	font-size: 24rpx;
	font-family: PingFangSC-Regular, PingFang SC;
	font-weight: 400;
	color: rgba(255, 255, 255, 1);
	line-height: 33rpx;
	margin-top: 33rpx;
	display: block;
}
.pic2,
.pic3,
.pic4 {
	width: 100%;
	height: 1300rpx;
}
.discuss {
	width: 100%;
	height: 1650rpx;
	background: rgba(255, 255, 255, 1);
	border-radius: 60rpx;
	overflow: hidden;
}
.leaveWord {
	width: 670rpx;
	height: 88rpx;
	background: rgba(247, 246, 246, 1);
	border-radius: 44rpx;
	margin: 56rpx auto 40rpx;
}
.footer {
	width: 670rpx;
	font-size: 24rpx;
	font-family: PingFangSC-Regular, PingFang SC;
	font-weight: 400;
	color: rgba(255, 255, 255, 1);
	line-height: 33rpx;
	margin: 90rpx auto 55rpx;
}
.buy {
	width: 100%;
	height: 98rpx;
	background: rgba(255, 75, 102, 1);
	position: fixed;
	bottom: 0;
	line-height: 98rpx;
	text-align: center;
}
.buy text {
	font-size: 32rpx;
	font-family: PingFangSC-Medium, PingFang SC;
	font-weight: 500;
	color: rgba(255, 255, 255, 1);
	line-height: 45rpx;
}
.last {
	width: 100%;
	height: 98rpx;
	background: rgba(255, 75, 102, 1);
}

