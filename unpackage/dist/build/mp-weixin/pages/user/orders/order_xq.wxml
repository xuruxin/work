<view><cu-custom vue-id="1" bgColor="bg-gradual-redcolor" isBack="{{true}}" bind:__l="__l" vue-slots="{{['content']}}"><view slot="content">订单详情</view></cu-custom><block wx:if="{{datas}}"><view><view class="choose_add_over whitebg"><view class="addicon">配送地址</view><view class="add">{{datas.address.province_city_area}}</view><view class="addxq">{{datas.address.address}}</view><view class="addpeople">{{datas.address.name+" "+datas.address.phone}}</view></view><view class="lists"><view class="order"><view class="order_status"><block wx:if="{{datas.row.status==1}}"><text>待发货</text></block><block wx:if="{{datas.row.status==2}}"><text>待收货</text></block><block wx:if="{{datas.row.status==3}}"><text>待评价</text></block><block wx:if="{{datas.row.status==4}}"><text>完成</text></block><block wx:if="{{datas.row.status==5}}"><text>退款审核</text></block><block wx:if="{{datas.row.status==6}}"><text>退款完成</text></block><block wx:if="{{datas.row.status==7}}"><text>退货申请</text></block><block wx:if="{{datas.row.status==8}}"><text>退货完成</text></block><block wx:if="{{datas.row.status==9}}"><text>取消订单</text></block><block wx:if="{{datas.row.status==10}}"><text>退款中</text></block><block wx:if="{{datas.row.status==11}}"><text>退货中</text></block></view><view class="out_trade_no">商品信息</view></view><view class="goods"><view class="goodsmsg"><view class="goodstitle">{{datas.row.goods_name}}</view><view class="goodstyps"><block wx:if="{{datas.row.skuname}}"><text>{{datas.row.skuname}}</text></block></view><view class="goodsmoney">{{"￥"+datas.row.money}}</view></view><image src="{{datas.row.goods_thumbnail_image}}"></image></view></view><view class="box3 box5 whitebg"><view class="xiaoji yunfei">包邮</view><view class="gnum">运费</view></view><view class="box3 box5 whitebg"><view class="xiaoji shiji">实际支付：<text class="moneyicon">￥</text><text class="money">{{datas.row.endmoney}}</text></view><view class="gnum"></view></view><view class="box3 box5 box6 whitebg"><view class="xiaoji paytype">微信小程序支付<image src="/static/weixin-icon.png"></image></view><view class="gnum">支付方式</view></view><view class="box7 whitebg"><view>{{"订单编号："+datas.row.out_trade_no}}</view><view>支付方式：微信小程序支付</view><view>{{"支付时间："+datas.row.endtime}}</view><view>{{"创建时间："+datas.row.createtime}}</view></view><view style="height:150rpx;"></view><form report-submit data-event-opts="{{[['submit',[['saveformid',['$event']]]]]}}" bindsubmit="__e"><block wx:if="{{datas.row.status==2||datas.row.status==3}}"><view class="btns"><block wx:if="{{datas.row.status==2}}"><button class="btn nullButton2" form-type="submit" id="4" data-orderid="{{options.oid}}" data-event-opts="{{[['tap',[['shouhuo',['$event']]]]]}}" bindtap="__e">确认收货</button></block><block wx:if="{{datas.row.status==2}}"><button class="btn nullButton2" form-type="submit" data-kdcode="{{datas.row.kd_code}}" data-kuaidi="{{datas.row.kd_number}}" id="4" data-orderid="{{options.oid}}" data-event-opts="{{[['tap',[['check_express',['$event']]]]]}}" bindtap="__e">查看物流</button></block><block wx:if="{{datas.row.status==3}}"><button class="btn gray nullButton2" form-type="submit" data-closepage="1" data-jumptype="1" data-url="{{'/pages/user/pinglun/addpinglun?order_id='+options.oid+'&title='+datas.row.goods_name+'&image='+datas.row.goods_thumbnail_image}}" data-event-opts="{{[['tap',[['jumpTo',['$event']]]]]}}" bindtap="__e">评价商品</button></block></view></block></form></view></block></view>